{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node app.js",
    "dev": "dotenv -e .env nodemon app.js",
    "pulldb:mysql": "dotenv -e .env -- npx prisma db pull",
    "migratedev:mysql": "dotenv -e .env -- npx prisma migrate dev",
    "migratereset:mysql": "dotenv -e .env -- npx prisma migrate reset",
    "pushdb:mysql": "dotenv -e .env -- npx prisma dp push"
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^17.2.0",
    "dotenv-cli": "^10.0.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "eslint": "^9.31.0",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.11.1"
  }
}
